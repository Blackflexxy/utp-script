{
  "name": "UA Language Audio",
  "includeCustomFormatWhenRenaming": false,
  "specifications": [
    {
      "name": "Ukrainian Language",
      "implementation": "LanguageSpecification",
      "negate": false,
      "required": true,
      "fields": {
        "value": 32,
        "exceptLanguage": false
      }
    },
    {
      "name": "[Ukrainian] tag",
      "implementation": "ReleaseTitleSpecification",
      "negate": false,
      "required": false,
      "fields": {
        "value": "(?i)\\[(?!\\s*Subs)[^]]*\\b(Ukrainian)\\b[^]]*\\]"
      }
    },
    {
      "name": "UKR tag",
      "implementation": "ReleaseTitleSpecification",
      "negate": false,
      "required": false,
      "fields": {
        "value": "(?i)^(?:(?!\\|).)*?(?:\\d+x)?(UKR)(?![\\s/_-]*(?:SUB\\b))"
      }
    }
  ]
}